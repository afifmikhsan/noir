    private void cbCorelActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        if (cbCorel.isSelected() == true) {
            jtfCorel.setEditable(true);
        } else {
            jtfCorel.setEditable(false);
            jtfCorel.setText("");
        }
    }                                       

