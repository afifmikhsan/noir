    private void btnUlangActionPerformed(java.awt.event.ActionEvent evt) {                                         
        // TODO add your handling code here:
        txtJlhMouse.setEditable(false);
        txtJlhKeyboard.setEditable(false);
        txtJlhHeadset.setEditable(false);
        txtJlhFlashdisk.setEditable(false);
        cboxMouse.setSelected(false);
        cboxKeyboard.setSelected(false);
        cboxHeadset.setSelected(false);
        cboxFlashdisk.setSelected(false);
        txtJlhMouse.setText("");
        txtJlhKeyboard.setText("");
        txtJlhHeadset.setText("");
        txtJlhFlashdisk.setText("");
        lblTJlh.setText("0 porsi");
        lblTHrgMouse.setText("Rp. 0,-");

        lblTHrgKeyboard.setText("Rp. 0,-");

        lblTHrgHeadset.setText("Rp. 0,-");

        lblTHrgFlashdisk.setText("Rp. 0,-");

        lblTBayar.setText("Rp. 0,-");

    }                                        

