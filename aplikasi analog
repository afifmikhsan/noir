    private void jbJumlahBimbelActionPerformed(java.awt.event.ActionEvent evt) {                                               
        // TODO add your handling code here:
        int total = 0;
        if (cbWord.isSelected()){
            String word = jtfWord.getText();
            int jumlahWord = Integer.parseInt(word);
            int hargaWord = jumlahWord * 200000;
            total = total + hargaWord;
        }
        if (cbExcel.isSelected()){
            String excel = jtfExcel.getText();
            int jumlahExcel = Integer.parseInt(excel);
            int hargaExcel = jumlahExcel * 200000;
            total = total + hargaExcel;
        }
        if (cbPowerpoint.isSelected()){
            String powerpoint = jtfPowerpoint.getText();
            int jumlahPowerpoint = Integer.parseInt(powerpoint);
            int hargaPowerpoint = jumlahPowerpoint * 200000;
            total = total + hargaPowerpoint;
        }
        if (cbPhotoshop.isSelected()){
            String photoshop = jtfPhotoshop.getText();
            int jumlahPhotoshop = Integer.parseInt(photoshop);
            int hargaPhotoshop = jumlahPhotoshop * 350000;
            total = total + hargaPhotoshop;
        }
        if (cbCorel.isSelected()){
            String corel = jtfCorel.getText();
            int jumlahCorel = Integer.parseInt(corel);
            int hargaCorel = jumlahCorel * 300000;
            total = total + hargaCorel;
        }
        if (cbCad2d.isSelected()){
            String cad2d = jtfCad2d.getText();
            int jumlahCad2d = Integer.parseInt(cad2d);
            int hargaCad2d = jumlahCad2d * 350000;
            total = total + hargaCad2d;
        }
        if (cbCad3d.isSelected()){
            String cad3d = jtfCad3d.getText();
            int jumlahCad3d = Integer.parseInt(cad3d);
            int hargaCad3d = jumlahCad3d * 450000;
            total = total + hargaCad3d;
        }
        if (cbPhotoscape.isSelected()){
            String photoscape = jtfPhotoscape.getText();
            int jumlahPhotoscape = Integer.parseInt(photoscape);
            int hargaPhotoscape = jumlahPhotoscape * 250000;
            total = total + hargaPhotoscape;
        }
        if (cbOffice.isSelected()){
            String office = jtfOffice.getText();
            int jumlahOffice = Integer.parseInt(office);
            int hargaOffice = jumlahOffice * 500000;
            total = total + hargaOffice;
        }
        if (cbCadfull.isSelected()){
            String cadfull = jtfCadfull.getText();
            int jumlahCadfull = Integer.parseInt(cadfull);
            int hargaCadfull = jumlahCadfull * 700000;
            total = total + hargaCadfull;
        }
        jtfJumlahBimbel.setText(""+total);
    }                                              

    private void jtfPhotoscapeActionPerformed(java.awt.event.ActionEvent evt) {                                              
        // TODO add your handling code here:
    }                                             

    private void jbHapusActionPerformed(java.awt.event.ActionEvent evt) {                                        
        // TODO add your handling code here:
        jtfWord.setEditable(false);
        jtfWord.setText("");
        jtfExcel.setEditable(false);
        jtfExcel.setText("");
        jtfPowerpoint.setEditable(false);
        jtfPowerpoint.setText("");
        jtfPhotoshop.setEditable(false);
        jtfPhotoshop.setText("");
        jtfCorel.setEditable(false);
        jtfCorel.setText("");
        jtfCad2d.setEditable(false);
        jtfCad2d.setText("");
        jtfCad3d.setEditable(false);
        jtfCad3d.setText("");
        jtfPhotoscape.setEditable(false);
        jtfPhotoscape.setText("");
        jtfOffice.setEditable(false);
        jtfOffice.setText("");
        jtfCadfull.setEditable(false);
        jtfCadfull.setText("");
        jtfJumlahBimbel.setEditable(false);
        jtfJumlahBimbel.setText("");
    }                                       

