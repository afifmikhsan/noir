    private void btnJumlahActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        int JlhMouse = 0,
                JlhKeyboard = 0,
                JlhHeadset = 0,
                JlhFlashdisk = 0,
                HrgMouse = 0,
                HrgKeyboard = 0,
                HrgHeadset = 0,
                HrgFlashdisk = 0,
                THrgMouse, THrgKeyboard, THrgHeadset, THrgFlashdisk, TJlh, TBayar;
        if (cboxMouse.isSelected() == false && cboxKeyboard.isSelected() == false && cboxHeadset.isSelected() == false && cboxFlashdisk.isSelected() == false) {
        } else {
            if (cboxMouse.isSelected() == true) {
                if (txtJlhMouse.getText().length() == 0) {
                    txtJlhMouse.requestFocus();
                } else if (txtJlhMouse.getText().matches("[1-9]")) {
                    JlhMouse = Integer.parseInt(txtJlhMouse.getText());
                    HrgMouse = 40000;
                    THrgMouse = JlhMouse * HrgMouse;
                    lblTHrgMouse.setText("Rp. " + THrgMouse + ",-");
                }
            }
            if (cboxKeyboard.isSelected() == true) {
                if (txtJlhKeyboard.getText().length() == 0) {
                    txtJlhKeyboard.requestFocus();
                } else if (txtJlhKeyboard.getText().matches("[1-9]")) {
                    JlhKeyboard = Integer.parseInt(txtJlhKeyboard.getText());
                    HrgKeyboard = 60000;
                    THrgKeyboard = JlhKeyboard * HrgKeyboard;
                    lblTHrgKeyboard.setText("Rp. " + THrgKeyboard + ",-");
                }
            }
            if (cboxHeadset.isSelected() == true) {
                if (txtJlhHeadset.getText().length() == 0) {
                    txtJlhHeadset.requestFocus();
                } else if (txtJlhHeadset.getText().matches("[1-9]")) {
                    JlhHeadset = Integer.parseInt(txtJlhHeadset.getText());
                    HrgHeadset = 70000;
                    THrgHeadset = JlhHeadset * HrgHeadset;
                    lblTHrgHeadset.setText("Rp. " + THrgHeadset + ",-");
                }
                if (cboxFlashdisk.isSelected() == true) {
                    if (txtJlhFlashdisk.getText().length() == 0) {
                        txtJlhFlashdisk.requestFocus();
                    } else if (txtJlhFlashdisk.getText().matches("[1-9]")) {
                        JlhFlashdisk = Integer.parseInt(txtJlhFlashdisk.getText());
                        HrgFlashdisk = 100000;
                        THrgFlashdisk = JlhFlashdisk * HrgFlashdisk;
                        lblTHrgFlashdisk.setText("Rp. " + THrgFlashdisk + ",-");
                    }
                }
            }

            TJlh = JlhMouse + JlhKeyboard + JlhHeadset + JlhFlashdisk;
            TBayar = HrgMouse*JlhMouse + HrgKeyboard*JlhKeyboard + HrgHeadset*JlhHeadset + HrgFlashdisk*JlhFlashdisk;
            lblTJlh.setText("" + TJlh + "Barang");
            lblTBayar.setText("Rp. " + TBayar + ",-");
        }

    }                                         

    private void cboxMouseActionPerformed(java.awt.event.ActionEvent evt) {                                          
        // TODO add your handling code here:
        if (cboxMouse.isSelected() == true) {
            txtJlhMouse.setEditable(true);
            txtJlhMouse.requestFocus();
        } else {
            txtJlhMouse.setEditable(false);
            txtJlhMouse.setText("");
            lblTHrgMouse.setText("Rp.0,-");
        }
    }                                         

